&soc {
	syncoam_sm100 {

		status = "ok";
	};

	earjack-debugger {
		status = "disable";
	};

	hall-bu52031nvx {
		status = "ok";
		revision = "...";
		hallic-pen-irq-gpio = <&msmgpio 144 0x00>;
		hallic-pouch-irq-gpio = <&msmgpio 102 0x00>;
		reg = <0x0 0x0>;
	};

	uei_irrc {
		status = "ok";
	    compatible = "uei,irrc";
	    uei,reset-gpio = <&msmgpio 78 0x00>;
	};


};
